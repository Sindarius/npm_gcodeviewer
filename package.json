{
	"name": "@sindarius/gcodeviewer",
	"version": "1.0.4",
	"description": "GCode Visualization Package",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build-old": "babel src -d dist",
		"build":  "babel src --out-file dist/sindarius-gcodeviewer.js"
	},
	"files": [
		"/dist"
	],
	"babel": {
		"presets": [
			"@babel/preset-env",
			["minify", {
				"builtIns": false,
				"evaluate": false,
				"mangle": true,
				"keepClassName" : true
			}]
		],
		"comments": false
		
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {}
	},
	"author": "Sindarius",
	"license": "ISC",
	"dependencies": {
		"@babylonjs/core": "^4.2.0",
		"@babylonjs/inspector": "^4.2.0",
		"@babylonjs/materials": "^4.2.0",
		"d3": "^7.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.15.0",
		"@babel/preset-env": "^7.15.0",
		"babel-preset-minify": "^0.5.1"
	}
}